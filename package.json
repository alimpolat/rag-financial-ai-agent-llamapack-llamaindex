{
  "name": "rag-financial-ai-agent",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "py:install": "python3 -m venv .venv && . .venv/bin/activate && pip install -r server/requirements.txt",
    "py:serve": ". .venv/bin/activate && uvicorn server.main:app --reload --host 0.0.0.0 --port ${PY_PORT:-8000}",
    "py:eval": ". .venv/bin/activate && python -m server.cli_eval --questions ${QUESTIONS:-questions.text} --top-k ${TOP_K:-6}",
    "py:test": ". .venv/bin/activate && python -m pytest server/tests/ -v",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "autoprefixer": "^10.4.19",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.453.0",
    "mammoth": "^1.6.0",
    "html-to-text": "^9.0.5",
    "llamaindex": "^0.3.13",
    "next": "^14.2.5",
    "pdf-parse": "^1.1.1",
    "postcss": "^8.4.41",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "typescript": "^5.5.4"
  }
}
